  "scripts": {
    "hotel": "hotel",
    "jss": "jss",
    "start": "npm-run-all --serial bootstrap:disconnected --parallel start:react start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components",
    "start:storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "serve": "npm-run-all --serial build-server:development --parallel start build-server:development:watch serve:nodemon",
    "serve:connected": "npm-run-all --serial build-server:development --parallel start:connected build-server:development:watch serve:nodemon",
    "serve:nodemon": "cd build.server && nodemon -r source-map-support/register --config server/nodemon.json index.js",
    "build-server:development:watch": "npx chokidar server/*.ts server.bundle/*.tsx src/** -c \"npm run build-server:development\"",
    "build-server:development": "cross-env-shell NODE_ENV=development \"webpack --mode development --config server/server.webpack.config.js\"",
    "build-server:production": "cross-env-shell NODE_ENV=production \"webpack --mode production --config server/server.webpack.config.js\"",
    "build": "npm-run-all --serial bootstrap:connected build:client build:serverbundle",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "react-scripts start",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"react-scripts build\"",
    "build:serverbundle": "cross-env-shell NODE_ENV=production \"webpack --mode production --config server.bundle/server.bundle.webpack.config.js\"",
    "build:storybook": "build-storybook -c .storybook -o storybookbuild -s ./public",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "graphql:update": "node -r babel-register ./scripts/update-graphql-fragment-data.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    "env"
  ]
}
