{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "noImplicitAny": false,
      "jsx": "react",
      "paths": {
          "*": [
              "../node_modules/*",
              "../src/types/*"
          ]
      }
  },
  "include": [
      "."
  ]
}